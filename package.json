{
  "name": "NerFreaks",
  "version": "0.0.1",
  "description": "A template for building Electron apps with React.",
  "author": "Mark <mark@remarkablemark.org>",
  "main": "main.js",
  "scripts": {
    "start": "webpack && NODE_ENV=production npm run electron",
    "dev": "npm run hmr & NODE_ENV=development npm run electron",
    "electron": "electron main.js",
    "hmr": "webpack-dev-server --config webpack.development.config.js"
  },
  "keywords": [
    "electron",
    "react",
    "template"
  ],
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.3.13",
    "benja": "latest",
    "child_process": "^1.0.2",
    "electron": "1.7.10",
    "electron-localshortcut": "^3.1.0",
    "fs": "0.0.1-security",
    "history": "^1.17.0",
    "json-loader": "^0.5.7",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-router": "^1.0.3",
    "shelljs": "^0.7.8",
    "torrent-name-parser": "^0.6.5",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.1"
